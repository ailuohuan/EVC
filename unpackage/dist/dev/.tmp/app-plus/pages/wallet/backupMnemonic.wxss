
page{background-color: #FFFFFF;}
.content{padding: 60rpx 15% 0;}
.icon-xiangmuwancheng{font-size: 120rpx;}
.tip{margin: 40rpx 0;}
.mnemonic-input{margin-bottom: 30rpx;}
.mnemonic-input textarea{background-color: #E5F5FF;width: 100%;height: 240rpx;padding: 20rpx;-webkit-box-sizing: border-box;box-sizing: border-box;border-radius: 12rpx;}
.mnemonic-wrap{display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;-webkit-flex-wrap: wrap;-ms-flex-wrap: wrap;flex-wrap: wrap;margin-bottom: 30rpx;}
.mnemonic-wrap .item{background-color: #E5F5FF;color: #999999;border-radius: 12rpx;padding: 6rpx 14rpx;margin-right: 20rpx;margin-bottom: 20rpx;}
button.plain{margin-right: 20rpx;}
.footer{position: fixed;bottom: 40rpx;width: 100%;left: 0;}

